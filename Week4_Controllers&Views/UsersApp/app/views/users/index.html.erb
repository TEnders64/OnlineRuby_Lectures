<table>
  <thead>
    <th>Name</th>
    <th>Email</th>
    <th>Password</th>
    <th>Created</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr class="unicorn">
      <td><a href="/users/<%= user.id %>"><%= user.name %></a></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><%= user.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New User', new_user_path %>
